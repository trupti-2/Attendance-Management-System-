        <script><![CDATA[var InstanceTimeZoneUtils  = Class.create();
InstanceTimeZoneUtils .prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
 getFormattedTime: function() {
        var checkInTime = this.getParameter("sysparm_time"); // Get check-in time from client
        if (!checkInTime) return "";

        var gdt = new GlideDateTime(checkInTime); // Convert to GlideDateTime
        return gdt.getDisplayValue(); // Return correctly formatted time in instance timezone

    }
  
});]]></script>
