        <script><![CDATA[// var AttendanceDataAjax = Class.create();
// AttendanceDataAjax.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

//     type: 'AttendanceDataAjax'
// });
var AttendanceDataAjax = Class.create();
AttendanceDataAjax.prototype = Object.extendsObject(AbstractAjaxProcessor, {

    getAttendanceRecords: function () {
        var employeeName = this.getParameter("sysparm_employee");
        var records = [];
        
        var gr = new GlideRecord("x_1596199_attend_attendance_data"); // Your Attendance Data table
        gr.addQuery("employee", employeeName);
        gr.query();

        while (gr.next()) {
            records.push({
                check_in_time: gr.getValue("check_in_time"),
                check_out_time: gr.getValue("check_out_time"),
                status: gr.getValue("status")
            });
        }

        return JSON.stringify(records);
    }
});
]]></script>
